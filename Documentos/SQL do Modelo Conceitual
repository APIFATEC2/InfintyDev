CREATE SCHEMA `GERENCIAMENTO TG` DEFAULT CHARACTER SET utf8 ;

CREATE TABLE aluno 
( 
 nome STRING,  
 emailFatec STRING PRIMARY KEY,  
 emailFornecido STRING,  
 idorientador INT,  
); 

CREATE TABLE orientador 
( 
 nome STRING,  
 emailFatec STRING PRIMARY KEY,  
); 

CREATE TABLE tg 
( 
 tipo STRING,  
 id INT PRIMARY KEY,  
 empresa_envolvida STRING,  
 problema STRING,  
 disciplina STRING,  
); 

CREATE TABLE entrega 
( 
 id INT PRIMARY KEY,  
 descricao STRING,  
 data_entrega STRING,  
); 

CREATE TABLE turma 
( 
 id INT PRIMARY KEY,  
 semestre_ano STRING,  
); 

CREATE TABLE Rel 
( 
 emailFatec STRING PRIMARY KEY,  
 id INT PRIMARY KEY,  
); 

CREATE TABLE Rel1 
( 
 emailFatec STRING PRIMARY KEY,  
 id INT PRIMARY KEY,  
); 

CREATE TABLE Rel11 
( 
 id INT PRIMARY KEY,  
 id INT PRIMARY KEY,  
); 

CREATE TABLE Rel111 
( 
 feedback STRING,  
 nota INT,  
 id INT PRIMARY KEY,  
 emailFatec STRING PRIMARY KEY,  
); 

ALTER TABLE aluno ADD FOREIGN KEY(idorientador) REFERENCES orientador (idorientador)
ALTER TABLE Rel ADD FOREIGN KEY(emailFatec) REFERENCES aluno (emailFatec)
ALTER TABLE Rel ADD FOREIGN KEY(id) REFERENCES tg (id)
ALTER TABLE Rel1 ADD FOREIGN KEY(emailFatec) REFERENCES aluno (emailFatec)
ALTER TABLE Rel1 ADD FOREIGN KEY(id) REFERENCES turma (id)
ALTER TABLE Rel11 ADD FOREIGN KEY(id) REFERENCES entrega (id)
ALTER TABLE Rel11 ADD FOREIGN KEY(id) REFERENCES turma (id)
ALTER TABLE Rel111 ADD FOREIGN KEY(id) REFERENCES entrega (id)
ALTER TABLE Rel111 ADD FOREIGN KEY(emailFatec) REFERENCES aluno (emailFatec)
